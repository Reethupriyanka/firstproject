num=int(input())
if(num==12):
	print("previous number is : 89")
if(num==89):
	print("next number is : 12")
if (num in range(12,90) and (num%10 > num//10)):
	for i in range(num+1,90):
		if((num+1) % 10 > (num+1) // 10):
			print("next number is :",num+1)
			break
		else:
			num=num+1
	for i in range(12,num):
		if((num-1) % 10 > (num-1) // 10):
			print("previous number is :",num-1)
			break
		else:
			num=num-1
else:
	print("Invalid numbers")

